<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>com.studentDemo.controller.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.studentDemo.controller.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StudentSearch</servlet-name>
        <servlet-class>com.studentDemo.controller.StudentSearch</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentSearch</servlet-name>
        <url-pattern>/StudentSearch</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StudentIns</servlet-name>
        <servlet-class>com.studentDemo.controller.StudentIns</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentIns</servlet-name>
        <url-pattern>/StudentIns</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StudentDel</servlet-name>
        <servlet-class>com.studentDemo.controller.StudentDel</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentDel</servlet-name>
        <url-pattern>/StudentDel</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StudentUpd</servlet-name>
        <servlet-class>com.studentDemo.controller.StudentUpd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentUpd</servlet-name>
        <url-pattern>/StudentUpd</url-pattern>
    </servlet-mapping>
</web-app>
